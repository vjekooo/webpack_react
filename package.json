{
  "name": "webpack_config",
  "version": "1.0.0",
  "description": "Webpack for React development",
  "main": "index.jsx",
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "dev": "NODE_ENV=development nodemon --watch webpack.config.js --exec \"webpack-dev-server\"",
    "prod": "NODE_ENV=production webpack -p",
    "test": "jest",
    "clean": "rm -rf ./dist"
  },
  "author": "Vjeko",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vjekooo/webpack_react"
  },
  "bugs": "https://github.com/vjekooo/webpack_react/issues",
  "keywords": [
    "react",
    "webpack"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.8",
    "eslint": "^4.15.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "img-loader": "^2.0.0",
    "jest": "^22.0.4",
    "json-loader": "^0.5.7",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.9",
    "postcss-loader": "^2.0.10",
    "react-dev-utils": "^4.2.1",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "standard": "^10.0.3",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "standard",
    "plugins": [
      "react"
    ],
    "rules": {
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error"
    }
  }
}

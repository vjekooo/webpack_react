	},
	"bugs": "https://github.com/vjekooo/webpack_react/issues",
	"keywords": [
		"react",
		"webpack"
	],
	"prettier": {
		"trailingComma": "none",
		"useTabs": true,
		"tabWidth": 2,
		"semi": false,
		"singleQuote": true,
		"printWidth": 120,
		"bracketSpacing": true,
		"arrowParens": "avoid"
	},

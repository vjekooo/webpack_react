		"babel-loader": "8.2.3",
		"babel-plugin-styled-components": "^1.13.3",
		"eslint": "^8.2.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-react": "1.1.7",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-jsx-a11y": "6.4.1",

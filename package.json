{
	"name": "webpack_config",
	"version": "2.1.0",
	"description": "Webpack for React development",
	"main": "index.jsx",
	"scripts": {
		"dev": "webpack serve --env mode=development",
		"build": "npm run clean | webpack --env mode=production",
		"build:analyze": "npm run build -- --env presets=analyze",
		"test": "jest -c jest.config.js",
		"clean": "rm -rf ./dist"
	},
